{
  "name": "vespa",
  "rules": {
    "field": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "field"
        },
        {
          "type": "SYMBOL",
          "name": "field_name"
        },
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "SYMBOL",
          "name": "_field_types"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "field_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "_field_properties": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "STRING",
              "value": ";"
            }
          ]
        }
      ]
    },
    "_field_types": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_type"
        },
        {
          "type": "SYMBOL",
          "name": "int_type"
        }
      ]
    },
    "int_type": {
      "type": "STRING",
      "value": "int"
    },
    "string_type": {
      "type": "STRING",
      "value": "string"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

